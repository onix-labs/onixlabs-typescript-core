{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ban-types": [
            false
        ],
        "member-access": [
            true,
            "check-accessor",
            "check-constructor",
            "check-parameter-property"
        ],
        "no-bitwise": [
            false
        ],
        "no-console": [
            false
        ],
        "no-construct": [
            false
        ],
        "no-namespace": [
            false
        ],
        "no-string-literal": [
            false
        ],
        "max-classes-per-file": [
            false,
            1
        ],
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true
        ],
        "typedef": [
            true,
            "call-signature",
            "property-declaration",
            "variable-declaration"
        ]
    },
    "rulesDirectory": []
}